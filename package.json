{
	"name": "article-loader",
	"version": "1.0.0",
	"description": "rss and html meta loader to add data to db. Used for datatattat",
	"homepage": "https://github.com/BhomasTakker/article-loader#readme",
	"bugs": {
		"url": "https://github.com/BhomasTakker/article-loader/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/BhomasTakker/article-loader.git"
	},
	"license": "ISC",
	"author": "tom.thomas.bakker@gmail.com",
	"type": "commonjs",
	"main": "dist/index.js",
	"scripts": {
		"build": "rimraf dist && npx tsc",
		"predev:start": "npm run build",
		"dev:start": "nodemon dist/index.js",
		"start": "node dist/index.js",
		"aws:stop": "pm2 stop article-loader",
		"preaws:start": "npm run build",
		"aws:start": "pm2 start dist/index.js --name article-loader --cron-restart=\"14 1 * * *\"",
		"aws:reload": "pm2 reload article-loader",
		"logs": "pm2 logs article-loader",
		"kill": "pm2 kill",
		"monitor": "pm2 monit article-loader",
		"postinstall": "tsc",
		"preserve": "npm run build",
		"serve": "concurrently \"npx tsc -w\" \"nodemon dist/index.js\"",
		"script": "npx ts-node scripts/remove-shorts.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"dependencies": {
		"cheerio": "^1.0.0",
		"concurrently": "^9.1.2",
		"cron": "^3.5.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"ioredis": "^5.6.0",
		"jest": "^30.1.3",
		"mongoose": "^8.9.6",
		"radio-browser-api": "^6.0.3",
		"rimraf": "^6.0.1",
		"rss-parser": "^3.13.0",
		"typescript": "^5.7.3"
	},
	"devDependencies": {
		"@types/express": "^5.0.0",
		"@types/jest": "^30.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.13.0",
		"nodemon": "^3.1.9",
		"ts-jest": "^29.4.1",
		"ts-node": "^10.9.2"
	}
}
